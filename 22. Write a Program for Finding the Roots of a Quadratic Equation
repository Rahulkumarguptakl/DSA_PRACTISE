#include<iostream>
#include <cmath>

using namespace std;
void friendroot(int a,int b,int c){
    if(a==0){
        cout<<"invalide";
        return;
    }
    int d=b*b-4*a*c;
    double sqrt_val = sqrt(abs(d));
     if (d > 0) {
        cout << "Roots are real and different \n";
        cout << (double)(-b + sqrt_val) / (2 * a) << "\n"
             << (double)(-b - sqrt_val) / (2 * a);
    }
    else if (d == 0) {
        cout << "Roots are real and same \n";
        cout << -(double)b / (2 * a);
    }
    else {
        cout << "Roots are complex \n";
        cout << -(double)b / (2 * a) << " + i"
             << sqrt_val / (2 * a) << "\n"
             << -(double)b / (2 * a) << " - i"
             << sqrt_val / (2 * a);
    }

}
int main(){
    int a=4,b=6,c=2;
    friendroot(a,b,c);
    return 0;
    
}
