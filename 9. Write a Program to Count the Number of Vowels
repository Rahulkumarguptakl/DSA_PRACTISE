#include<iostream>
using namespace std;
int main(){
    string str="chgvhhlbvjaFGJJK";
     int count=0;
     for(int i=0;str[i]!='\0'; i++){
         char ch = str[i];
    if(ch=='a'||ch=='e'||ch=='o'||ch=='u'||ch=='v'||ch=='A'||ch=='E'||ch=='O'||ch=='U'||ch=='V'){
        count++;
    
    }
     }
    cout<<"the number of vowels :  "<<count<<endl;
     
    return 0;
    
}
