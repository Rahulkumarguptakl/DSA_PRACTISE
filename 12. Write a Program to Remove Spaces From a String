#include<iostream>
using namespace std;
string remove_st(string str){
    string result="";
     for (char c : str){
         if(c!=' '){
             result+=c;
         }
     }
    return result;
    
}

int main(){
    string str="vifIUWBF FREA F A  FA";
    cout<<"Remove Spaces From a String "<<remove_st(str)<<endl;
    return 0;
}
