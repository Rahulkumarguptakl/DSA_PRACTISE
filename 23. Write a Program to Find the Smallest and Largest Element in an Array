#include<iostream>
using namespace std;
void findMinMax(int arr[],int n){
    int min =arr[0];
    int max =arr[0];
    for(int i=0;i<n;i++){
        if(arr[i]<min){
           min= arr[i];
        }if(arr[i]>max){
           max= arr[i];
        }
    }
    cout<<min<<endl;
    cout<<max<<endl;
}
int main(){
    int arr[]={2,5,6,3,9,3};
    int N=sizeof(arr)/sizeof(arr[0]);
    findMinMax(arr,N);
    return 0;
}
