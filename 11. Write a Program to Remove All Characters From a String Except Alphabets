#include<iostream>
#include<cctype>
#include<string>
using namespace std;
string removech(string str){
    string result="";
    for (char ch : str) {
        if(isalpha(ch)){
            result+=ch;
        }
    }
    return result;
    
}
int main(){
    string str="123Rahul@Tiger@%xtycu";
    
    cout<<" Remove All Characters From a String Except "<< removech(str)
    <<endl;
    
    return 0;
}

