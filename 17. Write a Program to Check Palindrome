#include<iostream>
using namespace std;
bool ispalidrom(int n){
  int temp=n;
  int ans=0;
    while (temp != 0) {
    ans = (ans * 10) + (temp % 10);
    temp = temp / 10;}
    return (ans==n); 
        }
 int main(){
    int n=21212;
    if(ispalidrom(n)){
        cout<<"yes";
    }
    else{
        cout<<"no";
    }
    return 0;
  };
