#include<iostream>
using namespace std;
int gcd(int a,int b){
    int result=min(a,b);
        
    while(result>0){
        if (a % result == 0 && b % result == 0){
            break;
        }
        result--;
    }
    return result;
}
int main(){
    int a=18;
    int b=12;
    cout<<"gcd "<<gcd(a,b)<<endl;
    return 0;
}
